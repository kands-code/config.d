{
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "json",
    "xml",
    "html",
    "rust",
    "go",
    "css",
    "javascript",
    "typescript",
    "yaml",
    "java",
    "python",
    "haskell"
  ],
  "codeLens.enable": true,
  "markdownlint.config": {
    "line-length": false,
    "list-indent": false
  },
  "yaml.format.enable": true,
  "xml.java.home": "/usr/lib/jvm/default/",
  "xml.server.workDir": "/home/kands/Templates/xml",
  "java.home": "/usr/lib/jvm/default",
  "java.enabled": true,
  "java.import.maven.enabled": true,
  "java.import.gradle.enabled": true,
  "java.configuration.maven.userSettings": "/home/kands/.local/lib/conf/settings.xml",
  "java.format.enabled": true,
  "java.autobuild.enabled": false,
  "texlab.path": "/usr/bin/texlab",
  "texlab.build.executable": "/usr/bin/xelatex",
  "texlab.build.onSave": true,
  "go.checkForUpdates": "disabled",
  "go.goplsPath": "/usr/bin/gopls",
  "rust-analyzer.enable": true,
  "rust-analyzer.lens.run": true,
  "rust-analyzer.server.path": "/home/kands/.local/share/rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",
  "python.linting.pylintEnabled": true,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": [
        "haskell",
        "lhaskell"
      ]
    },
    "lua": {
      "command": "lua-language-server",
      "filetypes": [
        "lua"
      ]
    }
  }
}
