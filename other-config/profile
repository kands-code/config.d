#! /usr/bin/env bash

# git gpg
export GPG_TTY=$(tty)
#mysql history
export MYSQL_HISTFILE="$XDG_DATA_HOME"/mysql_history
# path
export PATH="$PATH":"$HOME/.local/bin":"$HOME/.local/share/npm/bin":"$HOME/.local/share/cabal/bin":"$HOME/.local/share/cargo/bin":"$HOME/.local/share/gem/ruby/3.0.0/bin"
systemctl --user import-environment PATH
